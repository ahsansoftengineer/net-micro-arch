{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SBA.Hierarchy",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build SBA.Hierarchy",
      "program": "${workspaceFolder}/bin/Debug/net8.0/SBA.Hierarchy.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/api/Hierarchy/v1/swagger/index.html"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
