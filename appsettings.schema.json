{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "Logging": {
        "type": "object",
        "properties": {
          "LogLevel": {
            "type": "object",
            "properties": {
              "Default": { "type": "string" },
              "Microsoft.AspNetCore": { "type": "string" }
            },
            "required": ["Default", "Microsoft.AspNetCore"]
          }
        },
        "required": ["LogLevel"]
      },
      "AllowedHosts": { "type": "string" },
      "USE_HTTPS": { "type": "boolean" },
      "ASPNETCORE_URLS": { "type": "string" },
      "ASPNETCORE_HTTPS_PORT": { "type": "string" },
      "DOTNET_ENVIRONMENT": { "type": "string" },
      "ASPNETCORE_PROJECTZ_NAME": { "type": "string" },
      "ASPNETCORE_ROUTE_PREFIX": { "type": "string" },
      "ConnectionStrings": {
        "type": "object",
        "properties": {
          "SqlConnection": { "type": "string" }
        },
        "required": ["SqlConnection"]
      },
      "Clientz": {
        "type": "object",
        "properties": {
          "RabbitMQz": {
            "type": "object",
            "properties": {
              "Uri": { "type": "string" },
              "HostName": { "type": "string" },
              "VirtualHost": { "type": "string" },
              "Port": { "type": "integer" },
              "UserName": { "type": "string" },
              "Password": { "type": "string" }
            },
            "required": ["HostName", "Port"]

          },
          "Httpz": {
            "type": "object",
            "additionalProperties": {
              "type": "string",
              "format": "uri"
            }
          }
        },
        "required": ["RabbitMQz", "Httpz"]
      }
      
    },
    "required": [
      "Logging",
      "AllowedHosts",
      "USE_HTTPS",
      "ASPNETCORE_URLS",
      "ASPNETCORE_HTTPS_PORT",
      "DOTNET_ENVIRONMENT",
      "ASPNETCORE_PROJECTZ_NAME",
      "ASPNETCORE_ROUTE_PREFIX",
      "ConnectionStrings",
      "Clientz"
    ]
  }
  