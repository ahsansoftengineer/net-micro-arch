{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "Logging": {
        "type": "object",
        "properties": {
          "LogLevel": {
            "type": "object",
            "properties": {
              "Default": { "type": "string" },
              "Microsoft.AspNetCore": { "type": "string" }
            },
            "required": ["Default", "Microsoft.AspNetCore"]
          }
        },
        "required": ["LogLevel"]
      },
      "AllowedHosts": { "type": "string" },
      "USE_HTTPS": { "type": "boolean" },
      "ASPNETCORE_URLS": { "type": "string" },
      "ASPNETCORE_HTTPS_PORT": { "type": "string" },
      "DOTNET_ENVIRONMENT": { "type": "string" },
      "ASPNETCORE_PROJECTZ_NAME": { "type": "string" },
      "ASPNETCORE_ROUTE_PREFIX": { "type": "string" },
      "ConnectionStrings": {
        "type": "object",
        "properties": {
          "SqlConnection": { "type": "string" }
        },
        "required": ["SqlConnection"]
      },
      "ServiceHTTP": {
        "type": "object",
        "properties": {
          "CommandService": { "type": "string" }
        },
        "required": ["CommandService"]
      },
      "ServiceGRPC": {
        "type": "object"
      },
      "ServiceTCP": {
        "type": "object"
      }
    },
    "required": [
      "Logging",
      "AllowedHosts",
      "USE_HTTPS",
      "ASPNETCORE_URLS",
      "ASPNETCORE_HTTPS_PORT",
      "DOTNET_ENVIRONMENT",
      "ASPNETCORE_PROJECTZ_NAME",
      "ASPNETCORE_ROUTE_PREFIX",
      "ConnectionStrings",
      "ServiceHTTP",
      "ServiceGRPC",
      "ServiceTCP"
    ]
  }
  